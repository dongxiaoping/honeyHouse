<view>
  <view class='headView'>
    <open-data class='icon' mode='aspectFit' type='userAvatarUrl'></open-data>
    <view class='icon' />
    <view style='margin-left:280rpx;margin-top:50rpx;font-size:50rpx;font-weight:bold;'>
      <open-data type="userNickName"></open-data>
    </view>
  </view>
  <view catchtap="eventMoney" class="function_item">
    <view class="title">账户余额</view>
    <view style='margin-left:{{windowWidth/3}}px;'> {{userInfo.amount}} 元</view>
  </view>


  <view catchtap="eventToOrderPage" class="function_item" style="border-top:20rpx solid #F7F7F7;">
    <view class="title">我的订单</view>
  </view>

  <view catchtap="eventContract" class="function_item">
    <view class="title">联系客服</view>
  </view>

    <view catchtap="obtainMoney" class="function_item">
      <view class="title">挣取的现金</view>
    </view>

  <view catchtap="testClearInfo" class="function_item">
    <view class="title">测试清空数据</view>
  </view>
</view>